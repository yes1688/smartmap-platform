# Development Environment Configuration
BUILD_ENV=development
GIN_MODE=debug

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=spatial_platform_dev
DB_USER=spatial_user
DB_PASSWORD=spatial_dev_password

# Application Configuration
APP_PORT=8080
NGINX_PORT=3000

# Ollama Configuration
OLLAMA_PORT=11434
OLLAMA_MODEL=llama2:7b

# CesiumJS Configuration (Get your token from https://ion.cesium.com/)
CESIUM_ACCESS_TOKEN=YOUR_CESIUM_ACCESS_TOKEN_HERE

# AI Configuration
AI_MAX_TOKENS=2048
AI_TEMPERATURE=0.7

# Voice Configuration
VOICE_ENABLED=true
VOICE_LANGUAGE=zh-TW

# Game Configuration
GAME_ITEM_SPAWN_INTERVAL=30s
GAME_MAX_ITEMS=10
GAME_COLLECTION_RADIUS=50

# Security Configuration
JWT_SECRET=your_jwt_secret_key_here
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Logging
LOG_LEVEL=debug
LOG_FORMAT=text